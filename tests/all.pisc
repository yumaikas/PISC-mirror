# Run at toplevel of PISC development directory
"./tests/" list-files-at [ ->name :fname
	[ $fname str-neq? ] :is-not
	
	"test_root.pisc" is-not
	"all.pisc" is-not and
	"boltdb.pisc" is-not and
	$fname ".pisc" str-ends? and [ 
	       ${ "./tests/" $fname } import
		2 [ "" print ] times
	]  when
] vec-each