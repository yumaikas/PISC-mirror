# Run at toplevel of PISC development directory
"./tests/" list-files-at [ ->name :fname
	[ $fname str-neq? ] :file-not?
	
	"test_root.pisc" file-not?
	"all.pisc" file-not? and
	"boltdb.pisc" file-not? and
	$fname ".pisc" str-ends? and [ 
	       ${ "./tests/" $fname } import
		2 [ "" print ] times
	]  when
] vec-each